{
  "expo": {
    "name": "Stem",
    "slug": "stem",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "stem",
    "icon": "./assets/icon.png",
    "developmentClient": {
      "silentLaunch": true
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1221"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.stem.ghanaEducation",
      "buildNumber": "1"
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1C1221"
      },
      "package": "com.stem.ghanaEducation",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "name": "Stem - Ghana Education Platform",
      "shortName": "Stem",
      "lang": "en",
      "scope": "/",
      "themeColor": "#1C1221",
      "backgroundColor": "#1C1221",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "preferRelatedApplications": false,
      "meta": {
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "black-translucent",
        "apple-mobile-web-app-title": "Stem",
        "mobile-web-app-capable": "yes"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
